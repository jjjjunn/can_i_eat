# .env.example
# 이 파일의 이름을 .env로 복사한 후, 각 환경에 맞는 실제 값으로 채워주세요.
# 주석 처리된 값은 예시이며, 실제 환경에 맞게 수정해야 합니다.

# --- Database Settings ---
# 로컬 개발용 SQLite 예시: DATABASE_URL="sqlite:///./test.db"
# GCP Cloud SQL (PostgreSQL) 예시: DATABASE_URL="postgresql+pg8000://<DB_USER>:<DB_PASSWORD>@<DB_HOST>:<DB_PORT>/<DB_NAME>"
DATABASE_URL=

# --- Google Cloud Settings ---
# Google Cloud Storage 버킷 이름
GCS_BUCKET_NAME=
# Google Cloud Vision API 및 Gemini API 키
GOOGLE_API_KEY=
# Google Cloud 서비스 계정 키 파일 경로 (설정된 경우)
# GOOGLE_APPLICATION_CREDENTIALS=

# --- AI Service Keys ---
# OpenAI API 키 (필요시)
# OPENAI_API_KEY=

# --- OAuth 2.0 Settings (Google) ---
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
# 예시: GOOGLE_REDIRECT_URI="http://localhost:8000/auth/google/callback" (백엔드 콜백)
GOOGLE_REDIRECT_URI=

# --- OAuth 2.0 Settings (Kakao) ---
KAKAO_REST_API_KEY=
KAKAO_CLIENT_SECRET=
# 예시: KAKAO_REDIRECT_URI="http://localhost:8000/auth/kakao/callback" (백엔드 콜백)
KAKAO_REDIRECT_URI=

# --- Application Settings ---
# FastAPI 백엔드 서버 주소 (Streamlit에서 호출 시 사용)
# 예시: API_URL="http://localhost:8000"
API_URL=
# Streamlit 프론트엔드 주소 (OAuth 리디렉션 시 사용)
# 예시: STREAMLIT_APP_URL="http://localhost:8501"
STREAMLIT_APP_URL=

# --- Security / JWT Settings ---
# JWT 서명에 사용할 시크릿 키 (강력한 랜덤 문자열 추천)
# 예: openssl rand -hex 32
JWT_SECRET_KEY=
JWT_ALGORITHM="HS256"

# FastAPI 세션 미들웨어용 시크릿 키
SESSION_SECRET_KEY=

# --- Log & App Behavior ---
LOG_LEVEL="INFO"
